{
  "name": "Isometric Contributions for coding",
  "version": "1.0.1",
  "description": "Renders an isometric pixel view of coding contribution graphs.",
  "content_scripts": [ {
    "css": [ "iso.css" ],
    "js": [ "jquery.min.js", "obelisk.min.js", "iso.js" ],
    "matches": [ "https://coding.net/*"]
  }],
  "permissions": [
    "storage"
  ],
  "icons" : {
    "48" : "icon-48.png",
    "128" : "icon-128.png"
  },
  "manifest_version": 2
}
